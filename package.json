{
  "dependencies": {
    "express": "^4.21.0",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "migration:init": "npx sequelize-cli init",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migration:db": "npx sequelize-cli db:migrate"
  }
}
